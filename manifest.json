{
    "manifest_version": 2,
    "name": "RBW Agent Helper",
    "version": "1.0.1",
    "description": "Call the Agent's attention to the '(create)' button when the ZenDesk Requester is not associated with an Organization.",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png"
    },
    "author": "Reland Boyle",
    "homepage_url": "https://github.com/GlorifiedBicycle",
    "content_scripts": [
        {
            "matches": [
                "*://*.zendesk.com/agent/*"
            ],
            "js": ["content.js"]
        }
    ]
    
}